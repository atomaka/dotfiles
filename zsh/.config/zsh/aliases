alias a="ls -la"
alias c="clear"
alias g="g" # overwrite g alias
alias ga="git add"
alias gap="git add --patch"
alias gc="git commit"
alias gca="git commit --amend"
alias gd="git diff"
alias gdc="git diff --cached"
alias gr="git reset"
alias mf="mkdir"
alias mw="tmux -f $HOME/.config/tmux/tmux.conf"
alias sz="source $ZDOTDIR/.zshrc"
alias v="vim -u $HOME/.config/vim/vimrc"
alias x="exit"
